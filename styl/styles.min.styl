@import '../css/_css/bootstrap.min.css'
@import '../css/_css/flipclock.min.css'

*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: OS
	line-height: 1.3
	color #fff
	background-color: rgb(12,12,12)
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	width 100%
	max-width 290px;
	margin  0 auto
	border: 0;
	height: 50px;
	line-height: 50px;
	background-color: rgb(169, 12, 4);
	box-shadow: 5px 5px 0 rgba(169, 12, 4, .2)
	font-family: AB
	color: rgb(255, 255, 255);
	text-transform: uppercase;
	text-align: center
	transition: all .3s ease
	&:focus
		color #fff
		text-decoration: none
	&:hover
		box-shadow: 0 0 0 rgba(169, 12, 4, 1)
		color #fff
.general-title
	font-size: 40px;
	font-family: ABL
	color: rgb(254, 254, 254);
	text-transform: uppercase;
	line-height: 1
	text-align: center
	text-shadow: 3.064px 2.571px 0px rgba(13, 10, 10, 0.41);
	margin-bottom: 40px
	span
		color #538ffe

